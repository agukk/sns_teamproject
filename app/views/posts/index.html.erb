

<% if user_signed_in? %>
<%= button_to "新規投稿", {controller: 'posts',action: 'new'},{method: :get} %>
<% end %>
<% @posts.each do |post| %>
    <tr>
        <td><%= post.content %></td>
        
        <td>  <%= post.user_id%></td>

        <td><%= link_to "編集" , edit_post_path(post)%></td>

        <td><%= link_to "削除", post_path(post),method: :delete%></td>
    </tr>
<% end %>

